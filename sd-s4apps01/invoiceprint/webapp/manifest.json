{
  "_version": "1.60.0",
  "sap.app": {
    "id": "sd.invoiceprint",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.14.5",
      "toolsId": "807ab6f9-ccc2-4531-aa9a-0ea2e349aef5"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zui_invoicereport_o4/srvd/sap/zui_invoicereport_o4/0001/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      },
      "PrintService": {
        "uri": "/sap/opu/odata4/sap/zui_prt_record_o4/srvd/sap/zui_prt_record_o4/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/ZUI_PRT_RECORD_O4/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "AuthorityService": {
        "uri": "/sap/opu/odata4/sap/zui_permission_access_o4/srvd/sap/zui_permission_access_o4/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/ZUI_PERMISSION_ACCESS_O4/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "intent1": {
          "semanticObject": "invoiceprint",
          "action": "display",
          "icon": "sap-icon://print",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.platform.cf": {
    "uisVersionNumber": "1.120"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sd.invoiceprint.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "Print": {
        "dataSource": "PrintService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "Authority": {
        "dataSource": "AuthorityService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "local": {
        "type": "sap.ui.model.json.JSONModel",
        "preload": true
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "InvoiceReportList",
          "target": "InvoiceReportList"
        }
      ],
      "targets": {
        "InvoiceReportList": {
          "type": "Component",
          "id": "InvoiceReportList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/InvoiceReport",
              "variantManagement": "Page",
              "navigation": {},
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "GridTable",
                    "selectionMode": "Multi",
                    "quickVariantSelection": {
                      "showCounts": true
                    }
                  },
                  "actions": {
                    "ListReportExt": {
                      "press": "sd.invoiceprint.ext.listReportExt.ListReportExt.onPrint",
                      "visible": true,
                      "enabled": "{local>/authorityCheck/button/Print}",
                      "requiresSelection": true,
                      "text": "{i18n>print}"
                    },
                    "reprint": {
                      "press": "sd.invoiceprint.ext.listReportExt.ListReportExt.onReprint",
                      "visible": true,
                      "enabled": "{local>/authorityCheck/button/Reprint}",
                      "requiresSelection": true,
                      "text": "{i18n>reprint}"
                    },
                    "delete": {
                      "press": "sd.invoiceprint.ext.listReportExt.ListReportExt.onDelete",
                      "visible": true,
                      "enabled": "{local>/authorityCheck/button/Clear}",
                      "requiresSelection": true,
                      "text": "{i18n>delete}"
                    }
                  },
                  "columns": {
                    "DataField::ShippingPoint": {
                      "width": "7rem"
                    },
                    "DataField::OverallSDProcessStatus": {
                      "width": "8rem"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "sd-s4apps01"
  }
}

