{
  "_version": "1.60.0",
  "sap.app": {
    "id": "pp.ofpartition",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.0",
      "toolsId": "b4f721d7-3501-4212-8004-9d46d49d95b9"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZUI_OFPARTITION_O2/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZUI_OFPARTITION_O2_ANNOTATION"
          ],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZUI_OFPARTITION_O2_ANNOTATION": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZUI_OFPARTITION_O2_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation"
      },
      "createPIR": {
        "uri": "/sap/opu/odata4/sap/ZUI_CREATEPIR_O4/srvd/sap/ZUI_CREATEPIR_O4/0001/",
        "type": "OData",
        "settings": {
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "AuthorityService": {
          "uri": "/sap/opu/odata4/sap/zui_permission_access_o4/srvd/sap/zui_permission_access_o4/0001/",
          "type": "OData",
          "settings": {
              "annotations": [],
              "localUri": "localService/ZUI_PERMISSION_ACCESS_O4/metadata.xml",
              "odataVersion": "4.0"
          }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ofpartition-display": {
          "semanticObject": "ofpartition",
          "action": "display",
          "icon": "sap-icon://request",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.17",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "pp.ofpartition.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "pir": {
        "dataSource": "createPIR",
        "preload": true,
        "settings": {}
      },
      "Authority": {
          "dataSource": "AuthorityService",
          "preload": true,
          "settings": {
              "synchronizationMode": "None",
              "operationMode": "Server",
              "autoExpandSelect": true,
              "earlyRequests": true
          }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "pp.ofpartition.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        }
      }
    },
    "rootView": {
      "viewName": "pp.ofpartition.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "pp-s4apps01"
  },
  "sap.platform.cf": {
    "ui5VersionNumber": "1.120.18"
  }
}
